{
  "version": 2,
  "builds": [
    {
      "src": "rag_interface.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "rag_interface.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "KDBAI_ENDPOINT": "@kdbai_endpoint",
    "KDBAI_API_KEY": "@kdbai_api_key",
    "SUPABASE_KEY": "@supabase_key"
  }
}